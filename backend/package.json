{
  "name": "backend",
  "version": "1.0.0",
  "description": "This is the backend side of User management system",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "cd frontend && npm run start",
    "server-install": "npm install",
    "client-install": "cd frontend && npm install",
    "install-all": "concurrently \"npm run server-install\"  \"npm run client-install\"",
    "dev": "concurrently \"npm run server\"  \"npm run client\"",
    "heroku-postbuild": "cd frontend && npm install && npm run build"
  },
  "keywords": [
    "User",
    "management",
    "system",
    "NodeJS",
    "MongoDB",
    "Angular",
    "Express"
  ],
  "author": "Prajakta Bagde",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.3",
    "nodemon": "^2.0.12",
    "path": "^0.12.7"
  }
}
